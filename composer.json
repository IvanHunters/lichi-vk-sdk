{
  "name": "lichi/vk-sdk",
  "type": "library",
  "description": "Core for lichi Framework",
  "keywords": ["Lichi","lichi", "VK SDK", "vk sdk", "vk-sdk"],
  "homepage": "https://github.com/IvanHunters/lichi-vk-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Okhotnikov",
      "email": "ohotnikovivan@yandex.ru"
    }
  ],
  "minimum-stability": "dev-main",
  "prefer-stable": true,
  "require": {
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Lichi\\Vk\\": "src/",
      "Lichi\\Vk\\Sdk\\": "src/sdk"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "vimeo/psalm": "^4.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}
